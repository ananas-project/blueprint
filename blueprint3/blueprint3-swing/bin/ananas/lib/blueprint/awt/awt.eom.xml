<?xml version="1.0" encoding="UTF-8"?>
<eom xmlns="xmlns:ananas:blueprint:loader:eom">

	<namespace uri="xmlns:ananas:blueprint:swing" enableExport="true">


		<property key="myCtrlPackage" value="ananas.lib.blueprint.awt" />
		<property key="myTargetPackage" value="java.awt" />


		<property key="namespace:uri" value="::::awt" />
		<property key="namespace:defaultPrefix" value="awt" />
		<property key="class:javaName" value="xxx" />

		<property key="class:controllerClass" value="$(myCtrlPackage).C$(class:javaName)" />
		<property key="class:targetClass" value="$(myTargetPackage).$(class:javaName)" />



		<class name="Component" isElement="true" extends="java.lang.Object">
			<attribute name="test" />
			<element name="test" type="java.lang.Object" />
		</class>

		<class name="Container" isElement="true" extends="#Component">
			<attribute name="test" />
			<element name="test" type="java.lang.Object" />
		</class>

		<class name="Window" isElement="true" extends="#Container"
			targetClass="java.awt.Window" controllerClass="ananas.lib.blueprint.awt.CWindow">
			<attribute name="test" />
			<element name="test" type="java.lang.Object" />
		</class>

		<class name="Frame" isElement="true" extends="Window" javaName="Frame"
			localName="Frame">
			<attribute name="test" />
			<element name="test" type="java.lang.Object" />
			<element name="test2" type="#Container" />
		</class>

	</namespace>

</eom>
