<?xml version="1.0" encoding="UTF-8"?>
<reflect xmlns="xmlns:ananas:blueprint:loader:reflect">

	<namespace name="xmlns:ananas:blueprint:swing">


		<property key="myCtrlPackage" value="ananas.lib.blueprint.swing" />
		<property key="myTargetPackage" value="javax.swing" />


		<property key="ns:defaultPrefix" value="swing" />
		<property key="ns:localName" value="" />
		<property key="ns:setAttributeMethodNamePrefix" value="set_attribute_" />
		<property key="ns:appendChildMethodNamePrefix" value="append_child_" />
		<property key="ns:defaultControllerClass" value="$(myCtrlPackage).C$(ns:localName)" />
		<property key="ns:defaultTargetClass" value="$(myTargetPackage).$(ns:localName)" />



		<element name="JPanel" controllerClass="$(myCtrlPackage).CJPanel"
			targetClass="$(myTargetPackage).JPanel" />


		<element name="JFrame" />
		<element name="JLabel" />


	</namespace>

</reflect>
