<?xml version="1.0" encoding="UTF-8"?>
<eom xmlns="xmlns:ananas:blueprint:loader:eom">

	<namespace name="xmlns:ananas:blueprint:swing">


		<property key="myCtrlPackage" value="ananas.lib.blueprint.awt" />
		<property key="myTargetPackage" value="java.awt" />


		<property key="ns:defaultPrefix" value="awt" />
		<property key="ns:localName" value="xxx" />
		<property key="ns:setAttributeMethodNamePrefix" value="set_attribute_" />
		<property key="ns:appendChildMethodNamePrefix" value="append_child_" />
		<property key="ns:defaultControllerClass" value="$(myCtrlPackage).C$(ns:localName)" />
		<property key="ns:defaultTargetClass" value="$(myTargetPackage).$(ns:localName)" />



		<class name="Component" isElement="true" extends="java.lang.Object">
			<attribute name="test" />
			<element name="test" type="java.lang.Object" />
		</class>

		<class name="Container" isElement="true" extends="Component">
			<attribute name="test" />
			<element name="test" type="java.lang.Object" />
		</class>

		<class name="Window" isElement="true" extends="Container">
			<attribute name="test" />
			<element name="test" type="java.lang.Object" />
		</class>

		<class name="Frame" isElement="true" extends="Window">
			<attribute name="test" />
			<element name="test" type="java.lang.Object" />
		</class>

	</namespace>

</eom>
