<?xml version="1.0" encoding="UTF-8"?>
<mapping xmlns="xmlns:ananas:blueprint:eom">

	<namespace name="xmlns:ananas:blueprint:eom">

		<!-- properties -->

		<property name="defaultTargetClassPackage" value="eom" />
		<property name="defaultControllerClassPackage" value="eom" />

		<property name="ns:uri" value="xmlns:ananas:blueprint:eom" />
		<property name="ns:defaultPrefix" value="eom" />
		<property name="ns:localName" value="$(localName)" />
		<property name="ns:defaultAttributeTargetClass" value="eom" />
		<property name="ns:defaultElementTargetClass" value="eom" />
		<property name="ns:defaultAttributeControllerClass" value="eom" />
		<property name="ns:defaultElementControllerClass" value="eom" />

		<!-- classes -->

		<class name="namedableType">
			<attribute name="name" type="xs:string" />
		</class>

		<class name="mappingType">
			<child type="namespaceType" />
		</class>

		<class name="namespaceType" extends="namedableType">
			<element name="class" />
			<element name="property" />
			<element name="element" />
		</class>

		<class name="attributeType" extends="namedableType">
			<attribute name="type" type="xs:string" />
		</class>

		<class name="classType" extends="namedableType">
			<attribute name="extends" type="xs:string" />
			<attribute name="className" type="xs:string" />
			<element name="attribute" />
			<element name="child" />
		</class>

		<class name="elementType" extends="namedableType">
			<attribute name="type" type="xs:string" />
		</class>

		<class name="propertyType" extends="namedableType">
			<attribute name="value" />
		</class>

		<class name="childType" extends="namedableType">
		</class>

		<!-- elements -->

		<element name="mapping" type="mappingType" />
		<element name="namespace" type="namespaceType" />
		<element name="attribtue" type="attributeType" />
		<element name="class" type="classType" />
		<element name="element" type="elementType" />
		<element name="property" type="propertyType" />
		<element name="child" type="childType" />

	</namespace>

</mapping>
